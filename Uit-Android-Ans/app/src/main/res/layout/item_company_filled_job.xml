<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:id="@+id/adminMemberLayout"
    android:layout_marginTop="@dimen/_8sdp"
    android:background="@drawable/white_curved_background">


    <ImageView
        android:id="@+id/menYellow"
        android:layout_width="60dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:paddingLeft="@dimen/_8sdp"
        android:src="@drawable/noah"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/recruiter"
        android:layout_width="40dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:paddingLeft="@dimen/_8sdp"
        android:src="@drawable/recruiter"
        app:layout_constraintBottom_toBottomOf="@+id/menYellow"
        app:layout_constraintStart_toStartOf="@+id/menYellow"
        app:layout_constraintTop_toTopOf="@+id/menYellow" />

    <TextView
        android:id="@+id/applicantAccount"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/poppins_medium"
        android:gravity="left"
        android:text="@string/applicantAccount"
        android:textColor="@color/blackShadeOne"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/menYellow"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/diversity2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/poppins_medium"
        android:gravity="left"
        android:text="@string/uiDesign"
        android:textColor="@color/grayShadeThree"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/menYellow"
        app:layout_constraintTop_toBottomOf="@+id/applicantAccount" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="left"
        android:text="@string/seniorProductDesignerAndDeveloper"
        android:textColor="@color/app_color"
        android:textSize="@dimen/UITMediumTextSize"
        app:layout_constraintStart_toStartOf="@+id/menYellow"
        app:layout_constraintTop_toBottomOf="@+id/menYellow" />

    <TextView
        android:id="@+id/airBnb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:gravity="left"
        android:text="@string/uiDesign"
        android:textColor="@color/grayShadeFour"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintStart_toStartOf="@+id/welcome"
        app:layout_constraintTop_toBottomOf="@+id/welcome" />

    <TextView
        android:id="@+id/sanFrancisco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:gravity="left"
        android:text="@string/sanFrancisco"
        android:textColor="@color/grayShadeFour"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintStart_toStartOf="@+id/airBnb"
        app:layout_constraintTop_toBottomOf="@+id/airBnb" />


    <TextView
        android:id="@+id/yellow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:background="@drawable/applicant_border"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:paddingLeft="26dp"
        android:paddingTop="4dp"
        android:paddingRight="26dp"
        android:paddingBottom="4dp"
        android:text="@string/filled"
        android:textColor="@color/white"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintBottom_toBottomOf="@+id/sanFrancisco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sanFrancisco" />

    <TextView
        android:id="@+id/dollar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/white_background"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingTop="4dp"
        android:paddingRight="10dp"
        android:paddingBottom="4dp"
        android:text="$300k-$340k"
        android:textColor="@color/grayShadeFour"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sanFrancisco" />

    <TextView
        android:id="@+id/fullTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@drawable/white_background"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingTop="4dp"
        android:paddingRight="10dp"
        android:paddingBottom="4dp"
        android:text="@string/industry"
        android:textColor="@color/grayShadeFour"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintBottom_toBottomOf="@+id/dollar"
        app:layout_constraintStart_toEndOf="@+id/dollar"
        app:layout_constraintTop_toTopOf="@+id/dollar" />

    <TextView
        android:id="@+id/industry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@drawable/white_background"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingTop="4dp"
        android:paddingRight="10dp"
        android:paddingBottom="4dp"
        android:text="@string/fullTime"
        android:textColor="@color/grayShadeFour"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintBottom_toBottomOf="@+id/fullTime"
        app:layout_constraintStart_toEndOf="@+id/fullTime"
        app:layout_constraintTop_toTopOf="@+id/fullTime" />


</androidx.constraintlayout.widget.ConstraintLayout>